<UserControl
  x:Class="Mvvm.Apps.Views.MessageInteractionsView"
  xmlns="https://github.com/avaloniaui"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  xmlns:viewModels="using:Mvvm.Apps.ViewModels"
  mc:Ignorable="d"
  d:DesignWidth="800"
  d:DesignHeight="450"
  >
  <StackPanel>
    <Button Content="Message" Command="{Binding ShowMessageCommand}"/>
    <Button Content="Warning" Command="{Binding ShowWarningCommand}"/>
    <Button Content="Exception" Command="{Binding ShowExceptionCommand}"/>
    <Button Content="Question" Command="{Binding ShowQuestionCommand}"/>
  </StackPanel>
</UserControl>
